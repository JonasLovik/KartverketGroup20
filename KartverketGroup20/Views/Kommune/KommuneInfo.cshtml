@model KartverketGroup20.ViewModels.KommuneInfoViewModel

<h2>Kommune Informasjon</h2>

@if (Model != null)
{
    <p>Kommunenavn: @Model.Kommunenavn</p>
    <p>Kommunenummer: @Model.Kommunenummer</p>
    <p>Fylkesnavn: @Model.Fylkesnavn</p>
    <p>Samisk Forvaltningsområde: @(Model.SamiskForvaltningsområde ? "Ja" : "Nei")</p>
}
else
{
    <p>Ingen informasjon tilgjengelig</p>
}
